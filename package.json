{
  "name": "metalsmith-resume",
  "version": "0.0.1",
  "description": "A resume generator, built with Metalsmith",
  "main": "build.js",
  "engines": {
    "node": "5.9.0"
  },
  "scripts": {
    "start": "npm run browser_sync & NODE_ENV=development nodemon --exec 'npm run build && browser-sync reload'",
    "browser_sync": "browser-sync start --server _build --no-ui --no-notify",
    "build:html": "node build.js",
    "build:css": "postcss -c postcss.json -o _build/css/main.css --no-map.inline css/main.css",
    "build": "npm run build:html && npm run build:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lowmess/metalsmith-resume.git"
  },
  "keywords": [
    "metalsmith",
    "resume",
    "cv",
    "static",
    "site",
    "skeleton",
    "theme"
  ],
  "author": "Alec Lomas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lowmess/metalsmith-resume/issues"
  },
  "homepage": "https://github.com/lowmess/metalsmith-resume#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "browser-sync": "^2.12.10",
    "colors.css": "^3.0.0",
    "cssnano": "^3.7.1",
    "metalsmith": "^2.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-markdownit": "^0.3.0",
    "metalsmith-metadata": "0.0.4",
    "metalsmith-permalinks": "^0.5.0",
    "moment": "^2.13.0",
    "nodemon": "^1.9.2",
    "normalize.css": "^4.1.1",
    "postcss": "^5.0.21",
    "postcss-cli": "^2.5.2",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "postcss-nested": "^1.0.0",
    "postcss-uncss": "0.0.1",
    "pug": "^2.0.0-alpha8",
    "tachyons": "^4.0.1",
    "tachyons-border-colors": "^4.1.0",
    "tachyons-columns": "^1.0.0",
    "uncss": "^0.13.0"
  }
}
