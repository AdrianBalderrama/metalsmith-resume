{
  "name": "metalsmith-resume",
  "version": "0.0.1",
  "description": "A resume generator, built with Metalsmith",
  "main": "build.js",
  "engines": {
    "node": "5.9.0"
  },
  "scripts": {
    "start": "node build.js",
    "browser_sync": "browser-sync start --server _build --no-ui --no-notify",
    "develop": "npm run browser_sync & NODE_ENV=development nodemon --exec 'npm start && browser-sync reload'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lowmess/metalsmith-resume.git"
  },
  "keywords": [
    "metalsmith",
    "resume",
    "cv",
    "static",
    "site",
    "skeleton",
    "theme"
  ],
  "author": "Alec Lomas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lowmess/metalsmith-resume/issues"
  },
  "homepage": "https://github.com/lowmess/metalsmith-resume#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "browser-sync": "^2.12.10",
    "del": "^2.2.0",
    "metalsmith": "^2.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-markdownit": "^0.3.0",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-postcss": "^4.0.0",
    "moment": "^2.13.0",
    "nodemon": "^1.9.2",
    "normalize.css": "^4.1.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-discard-comments": "^2.0.4",
    "postcss-hexrgba": "^0.2.0",
    "postcss-import": "^8.1.2",
    "postcss-nested": "^1.0.0",
    "pug": "^2.0.0-alpha8",
    "tachyons-box-sizing": "^3.1.2",
    "tachyons-clears": "^2.2.0",
    "tachyons-colors": "^5.1.2",
    "tachyons-floats": "^2.0.4",
    "tachyons-font-family": "^4.1.2",
    "tachyons-font-weight": "^4.0.3",
    "tachyons-letter-spacing": "^2.1.2",
    "tachyons-links": "^3.0.1",
    "tachyons-spacing": "^5.0.10",
    "tachyons-type-scale": "^5.0.3",
    "tachyons-typography": "^2.3.0",
    "tachyons-widths": "^4.0.2"
  }
}
